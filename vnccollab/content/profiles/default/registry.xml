<?xml version="1.0"?>
<registry>
    <record name="vncollab.content.author_tab_list">
        <field type="plone.registry.field.List">
            <title>Author Dynamic Tabs URL list</title>
            <value_type type="plone.registry.field.TextLine">
                <title>Values</title>
            </value_type>
        </field>
        <value purge="false">
            <element>Following::@@user-following?userid={%uid}</element>
            <element>Followers::@@user-followers?userid={%uid}</element>
        </value>
    </record>
</registry>
