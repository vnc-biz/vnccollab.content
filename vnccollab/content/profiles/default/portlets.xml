<?xml version="1.0"?>
<portlets
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plone">
 
 <portlet
   addview="vnccollab.content.portlets.EtherpadsListPortlet"
   title="Etherpad List"
   description="A portlet displaying list of Etherpad pads."
   i18n:attributes="title;description">
   <for interface="plone.app.portlets.interfaces.IColumn" />
   <for interface="plone.app.portlets.interfaces.IDashboard" />
 </portlet>

  <portlet
    addview="vnccollab.content.portlets.GenericIframePortlet"
    title="Generic Iframe"
    description="Renders an external page inside an IFrame."
    i18n:attributes="title;description">
    <for interface="plone.app.portlets.interfaces.IColumn" />
    <for interface="plone.app.portlets.interfaces.IDashboard" />
  </portlet>

  <portlet
    addview="vnccollab.content.portlets.SpecialRSSPortlet"
    title="Special RSS"
    description="Renders list of RSS feed."
    i18n:attributes="title;description">
    <for interface="plone.app.portlets.interfaces.IColumn" />
    <for interface="plone.app.portlets.interfaces.IDashboard" />
  </portlet>

  <portlet
    addview="vnccollab.content.portlets.UsersBoxPortlet"
    title="Users Box"
    description="Lists users contributed to current container."
    i18n:attributes="title;description">
    <for interface="plone.app.portlets.interfaces.IColumn" />
    <for interface="plone.app.portlets.interfaces.IDashboard" />
  </portlet>

</portlets>
